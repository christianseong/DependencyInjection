package spring.di.entity;

public class NewlecExam implements Exam {

	private int math;
	private int kor;
	private int eng;
	private int com;
	@Override
	public int total() {
		super()
		// TODO Auto-generated method stub
		return math+kor+eng+com;
	}

	@Override 
	public float avg() {
		return total()/4.0f
	}

}
